<configuration debug="false">

    <include resource="org/springframework/boot/logging/logback/base.xml" />
    <include resource="member-appender.xml" />
    <include resource="path-appender.xml" />

            <!--    logger name이 member일때 적용할 appender를 등록한다.-->
    <logger name="member" level="DEBUG" >
        <appender-ref ref="member" />
    </logger>

            <!--    logger name이 path일때 적용할 appender를 등록한다.-->
    <logger name="path" level="INFO" >
        <appender-ref ref="path" />
    </logger>
</configuration>